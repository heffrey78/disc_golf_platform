{
  "name": "disc-golf-platform-backend",
  "version": "1.0.0",
  "description": "Backend for Disc Golf Platform",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/index.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/index.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
